#--------------------------------------------------------------------------------------------------------------
# build
#--------------------------------------------------------------------------------------------------------------
# Build the wind_river_base docker image
#--------------------------------------------------------------------------------------------------------------
docker/wind_river_base/build:
	docker build -f docker/wind_river/wind_river_base/wind_river_base.Dockerfile -t wind_river_base:0.0.0 .

#--------------------------------------------------------------------------------------------------------------
# run
#--------------------------------------------------------------------------------------------------------------
# Run to the wind_river_base docker image
#--------------------------------------------------------------------------------------------------------------
docker/wind_river_base/run: docker/wind_river_base/build
	docker run -it --entrypoint /bin/bash wind_river_base:0.0.0

#--------------------------------------------------------------------------------------------------------------
#
# End wind_river_base make targets
#
#--------------------------------------------------------------------------------------------------------------
